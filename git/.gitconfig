[color]
    ui = true

[core]
    excludesfile = ~/.gitignore

[alias]
    st = status
    ci = commit -a
    b = branch
    bt = branch --track
    co = checkout
    cob = checkout -b
    cobt = checkout -b --track
    d = diff
    l = log
    lp = log -p
    ls = log --stat
    lr = log --raw
    lg = log --graph
    lo = log --pretty=oneline
    adog = log --all --decorate --oneline --graph
    f = fetch
    ph = push
    pl = pull
    r = remote
    rs = remote show
    ra = remote add
    rd = remote del
    lsf = ls-files
    m = merge
    cl = clean -fd
    t = tag
    ui = update-index

[push]
    default = simple
    followTags = true
[pull]
    rebase = true
[merge]
    ff = only
[commit]
    gpgsign = true
    verbose = true

[init]
	defaultBranch = master
